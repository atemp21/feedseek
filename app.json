{
  "expo": {
    "name": "FeedSeek",
    "slug": "feed-seek",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#3A414B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow this app to use your location for nearby restaurants?"
      }
    },
    "android":{
      "permissions":["ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"]
    }
  }
}